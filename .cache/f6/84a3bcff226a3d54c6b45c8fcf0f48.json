x":"import React from 'react';\nimport axios from 'axios';\n\nimport { MovieCard } from '../movie-card/movie-card';\nimport { MovieView } from '../movie-view/movie-view';\n\nexport class MainView extends React.Component {\n\n    // Call the superclass constructor\n  // so React can initialize it\n  constructor() {\n    super();\n\n    // Initialize the state to an empty object so we can destructure it later\n    this.state = {\n      movies: null,\n      selectedMovie: null\n    };\n  }\n\n  // One of the \"hooks\" available in a React Component\n  componentDidMount() {\n    axios.get('<https://cineteca.herokuapp.com/movies>')\n      .then(response => {\n        console.log(response);\n        // Assign the result to the state\n        this.setState({\n          movies: response.data\n        });\n      })\n      .catch(function (error) {\n        console.log(error);\n      });\n  }\n\n  //go to movie view\n  onMovieClick(movie) {\n    this.setState({\n      selectedMovie: movie\n    });\n  }\n\n  // returning back to main view\n  getMainView(){\n    this.setState({\n      selectedMovie: null\n    });\n  }\n\n  render() {\n    // If the state isn't initialized, this will throw on runtime\n    // before the data is initially loaded\n    const { movies, selectedMovie } = this.state;\n\n    // Before the movies have been loaded\n    if (!movies) return <div className=\"main-view\"/>;\n\n    return (\n    <div className=\"main-view\">\n      {selectedMovie\n         ? <MovieView movie={selectedMovie} onClick={button => this.getMainView()}/>\n         : movies.map(movie => (\n           <MovieCard key={movie._id} movie={movie} onClick={movie => this.onMovieClick(movie)}/>\n         ))\n      }\n     </div>\n    );\n  }\n\n}\n"},"lineCount":null}},"error":null,"hash":"0e51b313d39ada4830a3299e1b199f77","cacheData":{"env":{}}}